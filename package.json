{
	"name": "GTagsForVSCode",
	"displayName": "gtagsc",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:c",
		"onLanguage:cpp"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "Global.SearchFile",
				"title": "Search File"
			},
			{
				"command": "Global.SearchSymbol",
				"title": "Search Symbol"
			}
		],
		"viewsContainers": {

		},
		"views": {

		},
		"menus": {

		},
		"configuration": {
			"type": "object",
			"title": "GTags Setting",
			"properties": {
				"gtagsSupport.globalPath": {
					"type": "string",
					"default": "D:\\glo663wb\\bin\\global.exe",
					"description": "global path"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
